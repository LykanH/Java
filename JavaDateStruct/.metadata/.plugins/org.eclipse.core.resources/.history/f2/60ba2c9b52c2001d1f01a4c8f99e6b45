	public void nextCreat(String s2) {
		char[] charArray = s2.toCharArray();
		int[] next = new int[s2.length()];
		//j为前缀末尾位置，i为后缀末尾位置
		int j = 0;
		next[0] = 0;
		for(int i = 1;i < next.length;i++) {
			while(j > 0 && charArray[i] != charArray[j]) {
				j = next[j-1];
			}
			if (charArray[i] == charArray[j]) {
				
			}
		}
	}
}
