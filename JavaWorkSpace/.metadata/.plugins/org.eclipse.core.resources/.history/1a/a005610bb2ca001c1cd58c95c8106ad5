package com.guigu.obj;

public class rectangleText {
	public static void main(String[] args) {
		rectangle rect1 = new rectangle();
		rect1.rectWidth = 8;
		rect1.rectHeight = 10;
	}
}

class rectangle{
	int rectWidth;
	int rectHeight;
	for(int i = 0;i < rectHeight;i++) {
		for(int j = 0;j < rectWidth;j++) {
			System.out.println("*");
			System.out.println();
		}
	}
}